id: {{ id }}
type: Card
properties:
  style:
    height: 100%
    minHeight: 450
    maxWIdth: 80%
blocks:
  - id: title
    type: Html
    properties:
      html: |
        <a href="{{ view}}" >
          <img style="width: 100%;" src="{{ image }}" alt="{{alt}}"/>
        </a>
        <h5 style="color:#1990FF;">
          {{ example }}
        </h5>
        <p>
          {{ desc }}
        </p>

  - id: git_button
    type: Button
    properties:
      title: View on GitHub
      icon: AiOutlineGithub
      type: link
      size: large
      block: true
      href: {{ git }}
