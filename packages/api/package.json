  "repository": {
    "type": "git",
    "url": "https://github.com/lowdefy/lowdefy.git"
  },
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./context/*": "./dist/context/*",
    "./routes/*": "./dist/routes/*"
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "yarn swc",
    "clean": "rm -rf dist",
    "prepare": "yarn build",
    "swc": "swc src --out-dir dist --config-file ../../.swcrc --delete-dir-on-start",
    "test": "yarn node --experimental-vm-modules $(yarn bin jest)"
  },
  "dependencies": {
    "@lowdefy/ajv": "4.0.0-alpha.11",
    "@lowdefy/helpers": "4.0.0-alpha.11",
    "@lowdefy/node-utils": "4.0.0-alpha.11",
    "@lowdefy/nunjucks": "4.0.0-alpha.11",
    "@lowdefy/operators": "4.0.0-alpha.11"
  },
  "devDependencies": {
    "@jest/globals": "27.5.1",
    "@lowdefy/operators-js": "4.0.0-alpha.11",
    "@swc/cli": "0.1.55",
    "@swc/core": "1.2.135",
    "@swc/jest": "0.2.17",
    "jest": "27.5.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
